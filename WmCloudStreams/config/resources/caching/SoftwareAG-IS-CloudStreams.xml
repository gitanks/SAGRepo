<ehcache     
    name="SoftwareAG.IS.CloudStreams">    
    <diskStore         
        path="./cacheStore/SoftwareAG-IS-CloudStreams"/>    
    <defaultCache         
        maxEntriesLocalHeap="0"/>    
    <cache         
        name="MetricAccumulatorCache"         
        eternal="true"         
        maxEntriesLocalHeap="10000"         
        copyOnWrite="true"         
        copyOnRead="true">        
        <persistence             
            strategy="none"/>        
        <terracotta             
            clustered="false"             
            consistency="STRONG">            
            <nonstop                 
                enabled="false"                 
                immediateTimeout="true"                 
                timeoutMillis="5000"/>        
        </terracotta>    
    </cache>    
    <cache         
        name="ClusterStatusCache"         
        eternal="true"         
